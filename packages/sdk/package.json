{
  "name": "@carrot-kpi/sdk",
  "license": "GPL-3.0",
  "author": "Federico Luzzi <fedeluzzi00@gmail.com>",
  "version": "1.0.0",
  "description": "An SDK for building applications on top of Carrot v1.",
  "main": "./dist/carrot-kpi-sdk.umd.js",
  "module": "./dist/carrot-kpi-sdk.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/carrot-kpi-sdk.es.js",
      "require": "./dist/carrot-kpi-sdk.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": "https://github.com/carrot-kpi/frontend-monorepo.git",
  "scripts": {
    "lint:code": "eslint --ext .tsx,.ts ./src && prettier --check ./src",
    "prettier:write": "prettier -w ./src/**",
    "build": "npm run clean && rollup -c rollup.config.js",
    "clean": "rimraf ./dist",
    "test": "echo 'No tests' && exit 0",
    "prepublishOnly": "npm run build",
    "size-limit": "size-limit",
    "analyze": "size-limit --why"
  },
  "dependencies": {
    "decimal.js-light": "^2.5.1",
    "multiformats": "^9.9.0"
  },
  "peerDependencies": {
    "ethers": "^5.7.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^14.1.0",
    "@size-limit/preset-small-lib": "^8.1.0",
    "@size-limit/webpack-why": "^8.1.0",
    "@types/rollup-plugin-peer-deps-external": "^2.2.1",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "ethers": "^5.7.0",
    "rimraf": "^3.0.2",
    "prettier": "^2.7.1",
    "rollup": "^2.79.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.34.0",
    "size-limit": "^8.1.0",
    "typescript": "^4.8.4"
  }
}
