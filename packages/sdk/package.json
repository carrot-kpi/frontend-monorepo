{
    "type": "module",
    "name": "@carrot-kpi/sdk",
    "license": "GPL-3.0-or-later",
    "author": "Federico Luzzi <fedeluzzi00@gmail.com>",
    "version": "1.25.0",
    "description": "An SDK for building applications on top of Carrot v1.",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js"
        }
    },
    "publishConfig": {
        "access": "public"
    },
    "repository": "https://github.com/carrot-kpi/v1-monorepo.git",
    "scripts": {
        "lint": "eslint --ext .tsx,.ts ./src && prettier --check './**/*.{json,md}'",
        "format": "eslint --fix --ext .tsx,.ts,.js ./ && prettier --write './**/*.{json,md}'",
        "build": "yarn clean && rollup -c rollup.config.mjs",
        "clean": "rimraf ./dist",
        "test": "jest",
        "size-limit": "size-limit",
        "release": "yarn publish"
    },
    "dependencies": {
        "decimal.js-light": "^2.5.1",
        "multiformats": "^9.9.0",
        "viem": "^0.3.24"
    },
    "peerDependencies": {
        "@carrot-kpi/shared-state": "*"
    },
    "devDependencies": {
        "@babel/core": "^7.21.8",
        "@babel/preset-env": "^7.21.5",
        "@babel/preset-typescript": "^7.21.5",
        "@carrot-kpi/shared-state": "*",
        "@jest/globals": "^29.5.0",
        "@rollup/plugin-commonjs": "^25.0.0",
        "@rollup/plugin-node-resolve": "^15.0.2",
        "@size-limit/preset-small-lib": "^8.1.0",
        "@types/jest": "^29.5.1",
        "@types/rollup-plugin-peer-deps-external": "^2.2.1",
        "babel-jest": "^29.5.0",
        "eslint-config-custom": "*",
        "jest": "^29.5.0",
        "rimraf": "^5.0.0",
        "rollup": "^3.21.7",
        "rollup-plugin-peer-deps-external": "^2.2.4",
        "rollup-plugin-typescript2": "^0.34.0",
        "size-limit": "^8.1.0",
        "ts-jest": "^29.1.0",
        "tsconfig": "*",
        "typescript": "^5.0.4"
    }
}
