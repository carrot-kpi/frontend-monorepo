{
    "name": "@carrot-kpi/host-frontend",
    "license": "GPL-3.0-or-later",
    "version": "0.11.0",
    "description": "Frontend for Carrot KPI protocol v1.",
    "main": "./dist/bootstrap.js",
    "module": "./dist/bootstrap.js",
    "types": "./dist/bootstrap.d.ts",
    "exports": {
        ".": {
            "import": "./dist/bootstrap.js"
        },
        "./styles.css": {
            "import": "./dist/styles.css"
        },
        "./shared-dependencies.json": {
            "import": "./shared-dependencies.json",
            "require": "./shared-dependencies.json"
        }
    },
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "lint": "eslint --ext .tsx,.ts ./src && prettier --check './**/*.{json,md}'",
        "format": "eslint --fix --ext .tsx,.ts,.js ./ && prettier --write './**/*.{json,md}'",
        "clean": "rimraf ./build && rimraf ./dist",
        "build": "yarn build:production",
        "build:production": "yarn clean && yarn prepare-fathom && GENERATE_SOURCEMAP=false craco build",
        "build:staging": "yarn clean && yarn prepare-fathom && GENERATE_SOURCEMAP=false STAGING=true craco build",
        "build:library": "yarn clean && yarn prepare-fathom && rollup -c rollup.config.mjs",
        "test": "react-scripts test --env=jsdom --watchAll=false --passWithNoTests",
        "size-limit": "size-limit",
        "prepare-fathom": "prepare-fathom",
        "config-react-env": "./scripts/config-react-env.js",
        "start:staging": "STAGING=true craco start",
        "start": "craco start"
    },
    "dependencies": {
        "@carrot-kpi/react": "*",
        "@carrot-kpi/sdk": "*",
        "@carrot-kpi/ui": "*",
        "class-variance-authority": "^0.6.1",
        "dayjs": "^1.11.8",
        "eth-provider": "^0.13.6",
        "ethereum-blockies-base64": "^1.0.2",
        "ethereum-provider": "^0.7.7",
        "react-redux": "8.1.1",
        "react-router-dom": "^6.14.0",
        "sanitize-html": "^2.11.0",
        "use-fathom-client": "^1.3.2",
        "viem": "^1.1.7"
    },
    "peerDependencies": {
        "@carrot-kpi/shared-state": "*",
        "@carrot-kpi/switzer-font": "^0.1.0",
        "@fontsource/ibm-plex-mono": "^5.0.3",
        "@tanstack/react-query": "^4.29.17",
        "i18next": "^23.2.3",
        "react": "^18.2.0",
        "react-i18next": "^13.0.1",
        "wagmi": "^1.3.1"
    },
    "devDependencies": {
        "@carrot-kpi/shared-state": "*",
        "@carrot-kpi/switzer-font": "^0.1.0",
        "@craco/craco": "^7.1.0",
        "@fontsource/ibm-plex-mono": "^5.0.3",
        "@react-spring/web": "^9.7.3",
        "@rollup/plugin-commonjs": "^25.0.2",
        "@rollup/plugin-json": "^6.0.0",
        "@rollup/plugin-node-resolve": "^15.1.0",
        "@rollup/plugin-replace": "^5.0.2",
        "@size-limit/file": "^8.2.6",
        "@tanstack/react-query": "^4.29.17",
        "@types/react": "^18.2.14",
        "@types/react-dom": "^18.2.6",
        "@types/react-router-dom": "^5.3.3",
        "@types/rollup-plugin-peer-deps-external": "^2.2.1",
        "@types/sanitize-html": "^2.9.0",
        "autoprefixer": "^10.4.14",
        "buffer": "^6.0.3",
        "cross-env": "^7.0.3",
        "dotenv": "^16.3.1",
        "eslint-config-custom": "*",
        "i18next": "^23.2.3",
        "postcss": "^8.4.24",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-i18next": "^13.0.1",
        "react-scripts": "^5.0.1",
        "react-use": "^17.4.0",
        "rimraf": "^5.0.0",
        "rollup": "^3.25.2",
        "rollup-plugin-peer-deps-external": "^2.2.4",
        "rollup-plugin-postcss": "^4.0.2",
        "rollup-plugin-typescript2": "^0.35.0",
        "size-limit": "^8.2.6",
        "tailwindcss": "^3.3.2",
        "tsconfig": "*",
        "typescript": "^5.1.3",
        "wagmi": "^1.3.0",
        "workbox-precaching": "^7.0.0",
        "workbox-routing": "^7.0.0"
    },
    "browserslist": {
        "production": [
            "chrome >= 67",
            "edge >= 79",
            "firefox >= 68",
            "opera >= 54",
            "safari >= 14"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
