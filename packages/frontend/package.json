{
    "name": "@carrot-kpi/host-frontend",
    "license": "GPL-3.0-or-later",
    "version": "0.41.0",
    "description": "Frontend for Carrot KPI protocol v1.",
    "main": "./dist/library-mode-entrypoint.js",
    "module": "./dist/library-mode-entrypoint.js",
    "types": "./dist/library-mode-entrypoint.d.ts",
    "exports": {
        ".": {
            "import": "./dist/library-mode-entrypoint.js"
        },
        "./styles.css": {
            "import": "./dist/styles.css"
        },
        "./shared-dependencies.json": {
            "import": "./shared-dependencies.json",
            "require": "./shared-dependencies.json"
        }
    },
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "lint": "eslint --ext .tsx,.ts ./src && prettier --check './**/*.{json,md}'",
        "format": "eslint --fix --ext .tsx,.ts,.js ./ && prettier --write './**/*.{json,md}'",
        "clean": "rimraf ./build && rimraf ./dist",
        "build": "yarn build:library",
        "build:production": "yarn clean && yarn prepare-fathom && ENVIRONMENT=prod craco build",
        "build:staging": "yarn clean && yarn prepare-fathom && ENVIRONMENT=staging craco build",
        "build:dev": "yarn clean && yarn prepare-fathom && ENVIRONMENT=dev craco build",
        "build:library": "yarn clean && yarn prepare-fathom && rollup -c rollup.config.mjs && tsc --project tsconfig.build.json",
        "build:standalone": "yarn clean && yarn prepare-fathom && craco build",
        "test": "react-scripts test --env=jsdom --watchAll=false --passWithNoTests",
        "size-limit": "size-limit",
        "prepare-fathom": "prepare-fathom",
        "config-react-env": "./scripts/config-react-env.js",
        "start:production": "yarn prepare-fathom && ENVIRONMENT=prod craco start",
        "start:staging": "yarn prepare-fathom && ENVIRONMENT=staging craco start",
        "start:dev": "yarn prepare-fathom && ENVIRONMENT=dev craco start",
        "start": "craco start",
        "synpress:run": "env-cmd -f ../../.env.e2e synpress run --browser chrome --configFile synpress.config.js",
        "test:e2e": "start-server-and-test 'yarn start:staging' http-get://127.0.0.1:3000 'yarn synpress:run'"
    },
    "dependencies": {
        "@carrot-kpi/react": "*",
        "@carrot-kpi/sdk": "*",
        "@carrot-kpi/ui": "*",
        "@faker-js/faker": "^8.4.0",
        "@synthetixio/synpress": "^3.7.2-beta.9",
        "class-variance-authority": "^0.7.0",
        "dayjs": "^1.11.10",
        "eth-provider": "^0.13.6",
        "ethereum-blockies-base64": "^1.0.2",
        "ethereum-provider": "^0.7.7",
        "jwt-decode": "^4.0.0",
        "react-redux": "9.1.0",
        "react-router-dom": "^6.21.3",
        "sanitize-html": "^2.11.0",
        "use-fathom-client": "^1.3.2",
        "viem": "^2.7.1"
    },
    "peerDependencies": {
        "@carrot-kpi/shared-state": "*",
        "@tanstack/react-query": "^5.18.1",
        "@carrot-kpi/switzer-font": "^0.1.0",
        "@fontsource/ibm-plex-mono": "^5.0.3",
        "i18next": "^23.5.1",
        "react": "^18.2.0",
        "react-i18next": "^13.4.1",
        "wagmi": "^2.5.5"
    },
    "devDependencies": {
        "@tanstack/react-query": "^5.18.1",
        "@carrot-kpi/shared-state": "*",
        "@carrot-kpi/switzer-font": "^0.1.0",
        "@commitlint/cli": "^18.6.0",
        "@commitlint/config-conventional": "^18.6.0",
        "@craco/craco": "^7.1.0",
        "@ethersproject/experimental": "^5.7.0",
        "@fontsource/ibm-plex-mono": "^5.0.3",
        "@react-spring/web": "^9.7.3",
        "@rollup/plugin-commonjs": "^25.0.7",
        "@rollup/plugin-image": "^3.0.3",
        "@rollup/plugin-json": "^6.0.1",
        "@rollup/plugin-node-resolve": "^15.2.3",
        "@rollup/plugin-replace": "^5.0.5",
        "@size-limit/file": "^11.0.2",
        "@types/cypress": "^1.1.3",
        "@types/react": "^18.2.48",
        "@types/react-dom": "^18.2.18",
        "@types/react-router-dom": "^5.3.3",
        "@types/rollup-plugin-peer-deps-external": "^2.2.4",
        "@types/sanitize-html": "^2.9.5",
        "autoprefixer": "^10.4.17",
        "buffer": "^6.0.3",
        "cross-env": "^7.0.3",
        "cssnano": "^6.0.3",
        "cypress": "^13.6.4",
        "dotenv": "^16.4.1",
        "env-cmd": "^10.1.0",
        "esbuild-loader": "^4.0.3",
        "eslint-config-custom": "*",
        "eslint-plugin-prettier": "^5.1.3",
        "husky": "^9.0.7",
        "i18next": "^23.8.2",
        "multiformats": "^13.0.1",
        "postcss": "^8.4.33",
        "prettier": "^3.2.4",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-i18next": "^13.5.0",
        "react-scripts": "^5.0.1",
        "react-use": "^17.5.0",
        "rimraf": "^5.0.5",
        "rollup": "^4.9.6",
        "rollup-plugin-esbuild": "^6.1.1",
        "rollup-plugin-peer-deps-external": "^2.2.4",
        "rollup-plugin-postcss": "^4.0.2",
        "rollup-plugin-typescript2": "^0.36.0",
        "size-limit": "^11.0.2",
        "start-server-and-test": "^2.0.3",
        "tailwindcss": "^3.4.1",
        "tsconfig": "*",
        "turbo": "^1.12.0",
        "typescript": "^5.3.3",
        "wagmi": "^2.5.5",
        "workbox-precaching": "^7.0.0",
        "workbox-routing": "^7.0.0"
    },
    "browserslist": {
        "production": [
            "chrome >= 67",
            "edge >= 79",
            "firefox >= 68",
            "opera >= 54",
            "safari >= 14"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
