{
  "name": "@carrot-kpi/react",
  "author": "Federico Luzzi",
  "version": "0.16.0",
  "license": "GPL-3.0-or-later",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "prettier --check ./src && eslint --ext .tsx,.ts ./src",
    "lint:fix": "prettier --write ./src && eslint --fix --ext .tsx,.ts ./src",
    "build": "pnpm clean && rollup -c rollup.config.js",
    "clean": "rimraf ./dist",
    "test": "echo 'No tests' && exit 0",
    "prepublishOnly": "pnpm build",
    "size-limit": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "@carrot-kpi/sdk": "workspace:../sdk",
    "ethers": "^5.7.0",
    "i18next": "^22.0.2",
    "react": "^18.2.0",
    "react-i18next": "^12.0.0",
    "wagmi": "^0.8.5"
  },
  "devDependencies": {
    "@carrot-kpi/sdk": "workspace:../sdk",
    "@rollup/plugin-commonjs": "^23.0.2",
    "@rollup/plugin-json": "^5.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@size-limit/preset-small-lib": "^8.1.0",
    "@size-limit/webpack-why": "^8.1.0",
    "@types/react": "^18.0.23",
    "@types/rollup-plugin-peer-deps-external": "^2.2.1",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "ethers": "^5.7.0",
    "i18next": "^22.0.6",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-i18next": "^12.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^3.2.3",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.34.0",
    "size-limit": "^8.1.0",
    "wagmi": "^0.8.5"
  }
}
