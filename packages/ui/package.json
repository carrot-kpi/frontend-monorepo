    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js"
        },
        "./styles.css": {
            "import": "./dist/styles.css"
        },
        "./tailwind-preset": {
            "import": "./dist/tailwind.preset.js",
            "require": "./dist/tailwind.preset.cjs"
        }
