    "devDependencies": {
        "@babel/core": "^7.19.3",
        "@ethersproject/address": "^5.7.0",
        "@fontsource/inter": "^4.5.15",
        "@rollup/plugin-commonjs": "^24.0.1",
        "@rollup/plugin-node-resolve": "^15.0.1",
        "@rollup/plugin-url": "^8.0.1",
        "@size-limit/preset-small-lib": "^8.1.0",
